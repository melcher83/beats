{
  "template": ["filebeat-7.0.0-alpha1-*"],
  "index_patterns": "filebeat-7.0.0-alpha1-*",
  "settings": {
    "number_of_shards": 1
  }
}
  "mappings": {
    "doc": {
      "_meta": {
        "version": "7.0.0-alpha1"
      },
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "string",
              "index": "not_analyzed"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {}
    }
  },
  "order": 1,
  "settings": {
    "index": {
      "mapping": {
        "total_fields": {
          "limit": 10000
        }
      },
      "number_of_routing_shards": 30,
      "number_of_shards": 3,
      "refresh_interval": "5s"
    }
  }
}
